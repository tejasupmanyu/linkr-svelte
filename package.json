{
  "name": "linkr",
  "description": "Svelte/Sapper application to share links",
  "version": "0.0.1",
  "author": {
    "name": "Tejas Upmanyu",
    "url": "https://tejasupmanyu.dev"
  },
  "scripts": {
    "start:dev": "sapper dev",
    "build": "yarn build:css && sapper build",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "watch:css": "postcss static/global.css -o static/index.css -w",
    "build:css": "NODE_ENV=production postcss static/global.css -o static/index.css",
    "dev": "run-p start:dev watch:css",
    "migrate:db": "node -r esm src/migration.js"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.1",
    "cookie-session": "^1.4.0",
    "dayjs": "^1.8.34",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "link-preview-generator": "^0.0.4",
    "lodash": "^4.17.19",
    "mysql2": "^2.1.0",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "sequelize": "^6.3.4",
    "sirv": "^0.4.0",
    "svelte-popover": "^2.0.8",
    "unique-names-generator": "^4.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1",
    "postcss-load-config": "^2.1.0",
    "postcss-nested": "^4.2.3",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-terser": "^5.3.0",
    "sapper": "^0.27.0",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^4.0.8",
    "tailwindcss": "^1.6.0"
  }
}
